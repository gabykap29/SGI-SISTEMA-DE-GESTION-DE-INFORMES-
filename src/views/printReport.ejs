                    <h6 id="departamento"></h6>
                    <h6 id="localidad"></h6>
                </div>
            </div>
            <div class="card col-5 " style="margin-left: 7%;">
                <div class="m-2">
                    <h6 id="Fecha"></h6>
                    <h6 id="Tipo"></h6>
                </div>
            </div>
    
            <div class="card col-11 mt-4">
                <p class="m-2" id="informe"></p>
            </div>
            <div class="card col-11 mt-4  mb-5">
                <div id="carouselExample" class="carousel slide">
                    <div class="carousel-inner">
                      <div class="carousel-item active">
                        <img src="/css/images/imagen-no-disponible.jpg" class="d-block w-100" alt="..." id="firstImg">
                      </div>
                      <div class="carousel-item">
                        <img src="/css/images/imagen-no-disponible.jpg" class="d-block w-100" alt="..." id="secondtImg">
